pulumi_content: |
  #!/bin/sh
  echo "Kubeconfig path is :" 
  echo $KUBECONFIG
  pulumi stack select dev
  pulumi up --yes
  pulumi stack output kubeconfig --show-secrets >$KUBECONFIG
pulumi_git_path: aws-ts-ecs-anywhere
pulumi_uninstall: |-
  #!/bin/sh
  pulumi stack select dev
  pulumi destroy --yes
  pulumi stack rm --yes
